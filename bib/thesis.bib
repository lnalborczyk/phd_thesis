@article{williams_resting_2017,
  title = {Resting {{Heart Rate Variability}}, {{Facets}} of {{Rumination}} and {{Trait Anxiety}}: {{Implications}} for the {{Perseverative Cognition Hypothesis}}},
  volume = {11},
