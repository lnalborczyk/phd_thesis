  file = {/Users/Ladislas/Zotero/storage/U7S9K9XE/Vickers - 2001 - The use of percentage change from baseline as an o.pdf;/Users/Ladislas/Zotero/storage/WGCSTNI2/Vickers - 2001 - The use of percentage change from baseline as an o.pdf}
