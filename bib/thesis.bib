  doi = {10.1371/journal.pone.0167490},
  author = {Cramer, Ang{\'e}lique O. J. and van Borkulo, Claudia D. and Giltay, Erik J. and van der Maas, Han L. J. and Kendler, Kenneth S. and Scheffer, Marten and Borsboom, Denny},
  month = dec,
