  file = {/Users/Ladislas/Desktop/Articles/Joorman, D'Avanzato - 2010.pdf;/Users/Ladislas/Zotero/storage/2CX28XGS/Joormann and D'Avanzato - 2010 - Emotion regulation in depression Examining the ro.pdf;/Users/Ladislas/Zotero/storage/K2B5WKMA/Joormann and D'Avanzato - 2010 - Emotion regulation in depression Examining the ro.pdf}
}

@article{Meier2012,
