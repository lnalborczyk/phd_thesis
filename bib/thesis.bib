  file = {/Users/Ladislas/Desktop/Articles/Grynberg, Maurage - 2014 - Pain and empathy the effect of self-oriented feelings on the detection of painful facial expressions.pdf;/Users/Ladislas/Zotero/storage/BUX2TAKU/Grynberg and Maurage - 2014 - Pain and Empathy The Effect of Self-Oriented Feel.pdf;/Users/Ladislas/Zotero/storage/KJ52DHLQ/Grynberg and Maurage - 2014 - Pain and Empathy The Effect of Self-Oriented Feel.pdf},
  pmid = {24983356}
