  journal = {Frontiers in Human Neuroscience},
  doi = {10.3389/fnhum.2017.00520},
  author = {Williams, DeWayne P. and Feeling, Nicole R. and Hill, LaBarron K. and Spangler, Derek P. and Koenig, Julian and Thayer, Julian F.},
  month = oct,
