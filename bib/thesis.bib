  file = {/Users/Ladislas/Zotero/storage/BK4BLU9D/Perugini et al. - 2014 - Safeguard Power as a Protection Against Imprecise .pdf;/Users/Ladislas/Zotero/storage/TLF5MQ58/Perugini et al. - 2014 - Safeguard Power as a Protection Against Imprecise .pdf;/Users/Ladislas/Zotero/storage/U8625SM6/Perugini et al. - 2014 - Safeguard Power as a Protection Against Imprecise .pdf;/Users/Ladislas/Zotero/storage/W6U8CYUE/Perugini et al. - 2014 - Safeguard Power as a Protection Against Imprecise .pdf}
