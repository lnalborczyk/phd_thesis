  keywords = {Depression,Network analysis,Dynamical systems,Fatigue,Insomnia,Pathogenesis,Simulation and modeling,Sleep},
  pages = {e0167490},
  file = {/Users/Ladislas/Zotero/storage/F3WJMJI6/Cramer et al. - 2016 - Major Depression as a Complex Dynamic System.pdf;/Users/Ladislas/Zotero/storage/LA3KEAUP/article.html}
}

@article{koster_understanding_2011,
  title = {Understanding Depressive Rumination from a Cognitive Science Perspective: The Impaired Disengagement Hypothesis},
